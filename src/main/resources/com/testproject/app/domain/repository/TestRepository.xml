<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.testproject.app.domain.repository.TestRepository">

  <select id="getInfo" resultType="com.testproject.app.domain.dto.ResponseDto">
	SELECT
	id as id,
	name as name,
	response_message as responseMessage
	FROM test
  </select>
 
</mapper>